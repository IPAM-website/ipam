{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/output.css --watch",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dockerapp": "docker build -t ipnova-app . && npm run dockerapp.run",
    "dockerapp.run": "docker run --env-file .env -p 80:80 --name ipnova ipnova-app",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "i18n-extract": "node_modules/.bin/localize-extract -s \"dist/build/*.js\" -f json -o src/locales/message.en.json",
    "i18n-translate": "npx localize-translate -s \"*.js\" -t src/locales/message.*.json -o dist/build/{{LOCALE}} -r ./dist/build",
    "prei18n-extract": "vite build",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --mode ssr --host",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@angular/compiler": "^16.2.2",
    "@angular/compiler-cli": "^16.2.2",
    "@builder.io/qwik": "^1.13.0",
    "@builder.io/qwik-city": "^1.13.0",
    "@eslint/js": "^9.27.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/eslint": "8.56.10",
    "@types/express": "^4.17.22",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "compression": "^1.8.0",
    "dotenv": "^16.5.0",
    "globals": "14.0.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "typescript": "5.1.6",
    "typescript-eslint": "^8.32.1",
    "vite": "5.4.9",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@angular/localize": "^16.2.2",
    "@eslint/config-array": "^0.19.2",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^3.0.2",
    "express": "4.20.0",
    "pnpm": "^10.11.0",
    "postgres": "^3.4.5",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "undici": "^7.9.0",
    "ws": "^8.18.2"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
